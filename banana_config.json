{
    "version": "1",
    "inputs": [
        {
            "name": "prompt",
            "description": "The prompt to feed to the model.",
            "type": "string",
            "required": true,
            "default": "Tell me about AI."
        },
        {
            "name": "temperature",
            "description": "The temperature to use.",
            "type": "number",
            "default": 0.5
        },
        {
            "name": "max_new_tokens",
            "description": "Generate at most this many new tokens in the response.",
            "type": "number",
            "default": 100
        }
    ]
}